<p>loads_this_page: members_only\app\views\users\new.html.erb</p>

<% flash.each do |key, value| %>
    <p class="alert alert-<%= key %>"><%= value %></p>
<% end %>

<%= form_for @user, url: {action: "log_in_check"} do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name%> <br>

  <%= f.label :password %>
  <%= f.password_field :password%> <br>

  <%= f.submit "Log in"%>
<% end %>
